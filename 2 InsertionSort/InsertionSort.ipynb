      "source": [
        "## Insertion Sort Algorithm"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "wQ0yMNCTWfYl"
      },
      "outputs": [],
      "source": [
        "def InsertionSort(A):\n",
        "  n = len(A)\n",
        "  for i in range(1, n):\n",
        "    key = A[i]\n",
        "    j = i-1\n",
        "    while j >= 0 and key < A[j]:\n",
        "      A[j+1] = A[j]\n",
        "      j -= 1\n",
        "    A[j+1] = key"
      ]
    }
  ],
