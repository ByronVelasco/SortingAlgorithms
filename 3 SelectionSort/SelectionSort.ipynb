      "source": [
        "## **Selection Sort Algorithm**"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 19,
      "metadata": {
        "id": "wQ0yMNCTWfYl"
      },
      "outputs": [],
      "source": [
        "def SelectionSort(A):\n",
        "  n = len(A)\n",
        "  for i in range(n-1):\n",
        "    min = i\n",
        "    for j in range(i+1, n):\n",
        "      if A[j] < A[min]:\n",
        "        min = j\n",
        "    A[i], A[min] = A[min], A[i]"
      ]
    }
  ],
